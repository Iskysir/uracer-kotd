#ifdef GL_ES
#define LOWP lowp
#define MED mediump
precision mediump float;
#else
#define LOWP  
#define MED 
#endif
uniform MED sampler2D u_texture0;
uniform float treshold;
uniform float tresholdD;
varying vec2 v_texCoords;
void main()
{
	vec4 tex = texture2D(u_texture0, v_texCoords);
	vec3 colors= clamp((tex.rgb - treshold)  * (tresholdD*tex.a),0.0,1.0);
 	gl_FragColor = vec4(colors,tex.a);
}